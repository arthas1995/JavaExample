<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.programme.persist.StudentMapper">

    <sql id="Base_Column_List">
        id, name, age, sex
    </sql>

    <insert id="save" parameterType="com.example.programme.domain.DO.StudentDO"
            keyProperty="id" useGeneratedKeys="true">
        INSERT INTO student
        (id, name, age, sex)
        VALUE
        (#{id}, #{name}, #{age},#{sex})
    </insert>
</mapper>